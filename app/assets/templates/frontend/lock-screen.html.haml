#lock-screen
  %h3 The app is locked.

  %form{"ng-submit" => "submitPasscodeForm()"}
    %input.form-control.mt-10{:type => 'password',
    "ng-model" => "formData.passcode", "autofocus" => "true",
    "placeholder" => "Enter Passcode"}
    %button.standard.ui-button.block.blue.mt-5{"type" => "submit"} Unlock
